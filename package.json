  "main": "server.min.js",
  "scripts": {
    "test": "jest",
    "start": "dotenv node ./server.min.js",
    "dev": "yarn run _clean:dev && run-p _watch:dev _launch:dev:wait",
    "build": "run-s _clean:dist _build:dist _copy:dist",
    "clean": "run-s _clean:dist _clean:dev",
    "_clean:dist": "rimraf ./.dist/",
    "_clean:dev": "rimraf ./.dev/",
    "_build:dev": "webpack --mode development",
    "_build:dist": "webpack --mode production",
    "_watch:dev": "yarn run _build:dev --watch",
    "_launch:dev:wait": "just-wait -p ./.dev/** && yarn run _launch:dev",
    "_launch:dev": "nodemon --inspect --watch ./.dev --exec \"node -r dotenv/config\" ./.dev/server.min.js",
    "_copy:dist": "cp-cli \"package.json\" \".dist/package.json\" && cp-cli \"yarn.lock\" \".dist/yarn.lock\""
  },
